<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background with rounded corners -->
  <rect width="512" height="512" rx="64" ry="64" fill="url(#bg)"/>

  <!-- Map pin icon -->
  <g transform="translate(256, 200)">
    <!-- Pin circle -->
    <circle cx="0" cy="0" r="60" fill="white"/>
    <circle cx="0" cy="0" r="20" fill="#1d4ed8"/>

    <!-- Pin point -->
    <polygon points="0,60 -30,120 30,120" fill="white"/>
  </g>

  <!-- Small location dots for map effect -->
  <circle cx="150" cy="350" r="8" fill="rgba(255,255,255,0.6)"/>
  <circle cx="380" cy="300" r="6" fill="rgba(255,255,255,0.4)"/>
  <circle cx="120" cy="280" r="4" fill="rgba(255,255,255,0.3)"/>
  <circle cx="400" cy="400" r="5" fill="rgba(255,255,255,0.5)"/>
</svg>